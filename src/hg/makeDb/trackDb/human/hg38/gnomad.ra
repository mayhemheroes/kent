    type bigBed 9 +
    itemRgb on
    html gnomadV3_1_1
    bigDataUrl /gbdb/hg38/gnomAD/v3.1/variants/genomes.bb
    priority 2
    labelFields name,rsId
    defaultLabelFields name
    sepFields AC_afr
    filterValues.annot pLoF,missense,synonymous,other
